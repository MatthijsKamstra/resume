<!doctype html>
<html>
	<head>

	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, user-scalable=no, minimal-ui">

	<title>::basics.name::</title>

	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.2.0/css/bootstrap.min.css">
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/octicons/2.0.2/octicons.min.css">

	<style type="text/css">
	::css::
	</style>

	</head>
	<body>
	<header id="header">
		<div class="container">
			<div class="row">
				<div class="col-sm-9 col-sm-push-3">
					<h1>
					::basics.name::
					</h1>
					<h2>
					::basics.label::
					</h2>
				</div>
			</div>
		</div>
	</header>

	<div id="content" class="container">

	<!-- contact -->
	<section id="contact" class="row">
		<aside class="col-sm-3">
			<h3>Contact</h3>
		</aside>
		<div class="col-sm-9">
			<div class="row">
				<div class="col-sm-6">
					<strong>Email</strong>
					<div class="email">
						::basics.email::
					</div>
				</div>
				<div class="col-sm-6">
					<strong>Phone</strong>
					<div class="phone">::basics.phone::</div>
				</div>

				<div class="col-sm-6">
					<strong>Website</strong>
					<div class="website">
						<a href="::basics.website::">::basics.website::</a>
					</div>
				</div>
			</div>
		</div>
	</section>

	<!-- about -->
	<section id="about" class="row">
		<aside class="col-sm-3">
			<h3>About</h3>
		</aside>
		<div class="col-sm-9">
			<p>::basics.summary::</p>
		</div>
	</section>

	<!-- profiles -->
	<section id="profiles" class="row">
		<aside class="col-sm-3">
			<h3>Profiles</h3>
		</aside>
		<div class="col-sm-9">
			<div class="row">
				::foreach profiles::
				<div class="col-sm-6">
					<strong class="network">
						::network::
					</strong>
					::if (username)::
					<div class="username">
						::if (url)::
						<div class="url">
							<a href="::url::">::username::</a>
						</div>
						::else::
							::username::
						::end::
					</div>
					::else::
						::if (url)::
						<div class="url">
							<a href="::url::">::url::</a>
						</div>
						::end::
					::end::
				</div><!-- /.col-sm-6 -->
				::end::
			</div>
	</section>


	<section id="work" class="row">
		<aside class="col-sm-3">
			<h3>Work</h3>
		</aside>
		<div class="col-sm-9">
			<div class="row">
				::foreach work::
				<div class="col-sm-12">
					<h4 class="strike-through">
						<span>::company::</span>
						<span class="date">
							::startDate:: — ::endDate::
						</span>
					</h4>
					<div class="website pull-right">
						<a href="::website::">::website::</a>
					</div>
					<div class="position">
						::position::
					</div>
					<div class="summary">
						<p>::summary::</p>
					</div>
					<h4>Highlights</h4>
					<ul class="highlights">
					::foreach highlights::
						<li class="bullet">::__current__::</li>
					::end::
					</ul>
				</div>
			::end::
			</div>
		</div>
	</section>


	<!-- volunteer -->

	<!-- education -->
	::if education::
	<section id="education" class="row">
		<aside class="col-sm-3">
			<h3>Education</h3>
		</aside>
		<div class="col-sm-9">
			<div class="row">
			::foreach education::
			<div class="col-sm-12">
				<h4 class="strike-through">
					<span>::institution::</span>
					<span class="date">
						::startDate:: — ::endDate::
					</span>
				</h4>
				<div class="area">
					::area::
				</div>
				<div class="studyType">
					::studyType::
				</div>
				<h4>Courses</h4>
				<ul class="courses">
				::foreach courses::
					<li>::__current__::</li>
				::end::
				</ul>
			</div>
			::end::
			</div>
		</div>
	</section>
	::end::


	<!-- awards -->
	<!-- publications -->

	<!-- skills -->
	::if skills::
	<section id="skills" class="row">
		<aside class="col-sm-3">
			<h3>Skills</h3>
		</aside>
		<div class="col-sm-9">
			<div class="row">
			::foreach skills::
			<div class="col-sm-6">
				<div class="name">
					<h4>::name::</h4>
				</div>
				::if keywords::
				<ul class="keywords">
					::foreach keywords::
					<li>::__current__::</li>
					::end::
				</ul>
				::end::
			</div>
			::end::
			</div>
		</div>
	</section>
	::end::

	<!-- languages -->
	::if languages::
	<section id="languages" class="row">
		<aside class="col-sm-3">
			<h3>Languages</h3>
		</aside>
		<div class="col-sm-9">
			<div class="row">
			::foreach languages::
			<div class="col-sm-6">
				<div class="language">
					<strong>::language::</strong>
				</div>
				<div class="fluency">::fluency::</div>
			</div>
			::end::
			</div>
		</div>
	</section>
	::end::

	<!-- interests -->
	::if interests::
	<section id="interests" class="row">
		<aside class="col-sm-3">
			<h3>Interests</h3>
		</aside>
		<div class="col-sm-9">
			<div class="row">
			::foreach interests::
			<div class="col-sm-6">
				<div class="name">
					<h4>::name::</h4>
				</div>
				::if keywords::
				<ul class="keywords">
					::foreach keywords::
					<li>::__current__::</li>
					::end::
				</ul>
				::end::
			</div>
			::end::
			</div>
		</div>
	</section>
	::end::


	<!-- references -->




	</div>

	</body>
</html>

